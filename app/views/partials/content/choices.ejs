<!-- Loops through array of current choices, and if greater than zero, displays them with an id as isbn that allows the API info to populate on the front end. -->

<div class="row">
	<% if (user.bookLikes.length > 0) { %>
		<h3 class="add-margin-2 center-align">Your selections:</h3>
		<% for (i=0; i< user.bookLikes.length; i++) { %>
			<div class="col s12 m4 request center-align" id="<%= user.bookLikes[i] %>"></div>
		<% } %>
	<% } %>
</div>


